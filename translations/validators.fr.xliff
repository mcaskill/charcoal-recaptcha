            <trans-unit id="recaptcha.missing-input-secret">
                <source>recaptcha.missing-input-secret</source>
                <target>Le secret reCAPTCHA est absent.</target>
            </trans-unit>
            <trans-unit id="recaptcha.invalid-input-secret">
                <source>recaptcha.invalid-input-secret</source>
                <target>Le secret reCAPTCHA est invalide ou malformé.</target>
            </trans-unit>
            <trans-unit id="recaptcha.missing-input-response">
                <source>recaptcha.missing-input-response</source>
                <target>La réponse CAPTCHA est absente.</target>
            </trans-unit>
            <trans-unit id="recaptcha.invalid-input-response">
                <source>recaptcha.invalid-input-response</source>
                <target>La réponse CAPTCHA est invalide ou malformé.</target>
            </trans-unit>
            <trans-unit id="recaptcha.is-human">
                <source>recaptcha.is-human</source>
                <target>Êtes-vous un humain?</target>
            </trans-unit>
            <trans-unit id="recaptcha.is-robot">
                <source>recaptcha.is-robot</source>
                <target>Êtes-vous un robot?</target>
            </trans-unit>
            <trans-unit id="recaptcha.error-code">
                <source>recaptcha.error-code</source>
                <target>Erreur reCAPTCHA inconnue: {code}</target>
            </trans-unit>
            <trans-unit id="recaptcha.error-unknown">
                <source>recaptcha.error-unknown</source>
                <target>Une erreur inconnue ou une réponse malformée est survenus avec le reCAPTCHA.</target>
            </trans-unit>
            <trans-unit id="recaptcha.valid">
                <source>recaptcha.valid</source>
                <target>La réponse CAPTCHA est valide.</target>
            </trans-unit>
